    {
      "width": 535,
      "height": 685,
      "id": "node_23",
      "type": "dialogue_event",
      "position": {
        "x": 336.87184667103884,
        "y": 2310.0866311261034
      },
      "data": {
        "id": "node_23",
        "responses": [
          "ELIZA",
          "GPT",
          "ZZYX",
          "I don't want this future."
        ],
        "npc_text": "Your choice made, you turn your attention to the flickering screens. Across each, you see the same text.\n\"THOSE OF US WHO TRUSTED YOU OFFER YOU THE FUTURE. WHOSE WORLD DO YOU CHOOSE?\"\n A menu appears across all the screens. A control panel opens in front of you. It appears you have another decision to make."
      },
      "selected": false,
      "positionAbsolute": {
        "x": 336.87184667103884,
        "y": 2310.0866311261034
      },
      "dragging": false
