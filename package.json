{
  "name": "threejs-es6-webpack-boilerplate",
  "version": "1.6.1",
  "description": "Boilerplate for Three.js projects set up with Babel for ES6 and compiled with webpack",
  "author": "Paul Graffam",
  "main": "app.js",
  "engines": {
    "node": ">=10.0"
  },
  "scripts": {
    "dev": "run-s dev:js",
    "build": "run-s build:dir build:js",
    "prebuild": "run-s clean",
    "clean": "rimraf build",
    "lint": "eslint src/js/",
    "webpack-watch": "webpack --env NODE_ENV=development --progress --color --watch --hot",
    "dev:js": " NODE_ENV=development webpack serve --env NODE_ENV=development",
    "build:dir": "copyfiles -a -u 2 src/public/assets/**/*.* build/",
    "build:js": "webpack --env NODE_ENV=prod --progress --color"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^18.6.4",
    "animejs": "^3.2.1",
    "assert": "^2.0.0",
    "autoprefixer": "^10.4.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "buffer": "^6.0.3",
    "capture-website": "^3.1.0",
    "chroma-js": "^2.1.2",
    "es6-promise": "^4.2.8",
    "fs": "^0.0.1-security",
    "natural": "^5.1.13",
    "optionator": "^0.9.1",
    "os": "^0.1.2",
    "path-browserify": "^1.0.1",
    "phantomjs-prebuilt": "^2.1.16",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "process": "^0.11.10",
    "randomcolor": "^0.6.2",
    "randomstring": "^1.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tailwindcss": "^3.0.17",
    "three": "^0.144.0",
    "three-spritetext": "^1.6.5",
    "toxiclibsjs": "^0.3.3",
    "troika-three-text": "^0.47.1",
    "troika-three-utils": "^0.47.0",
    "webshot-node": "^0.18.3",
    "wordnet-db": "^3.1.14"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^10.2.1",
    "copyfiles": "^2.4.1",
    "css-loader": "^5.2.0",
    "eslint": "^8.23.1",
    "eslint-plugin-react": "^7.32.2",
    "html-webpack-plugin": "^5.3.1",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^1.3.9",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^5.2.0",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.0.0-beta.1"
  },
  "optionalDependencies": {
    "win-node-env": "^0.6.1"
  }
}
